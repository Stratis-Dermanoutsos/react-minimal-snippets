{
    "name": "react-minimal-snippets",
    "displayName": "React Minimal Snippets",
    "description": "Extension for React snippets meant for minimalists",
    "version": "1.0.0",
    "publisher": "stratis-dermanoutsos",
    "author": "Stratis Dermanoutsos <stratis.dermanoutsos@gmail.com>",
    "icon": "images/logo.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/Stratis-Dermanoutsos/react-minimal-snippets.git"
    },
    "bugs": {
        "url": "https://github.com/Stratis-Dermanoutsos/react-minimal-snippets/issues"
    },
    "homepage": "https://github.com/Stratis-Dermanoutsos/react-minimal-snippets",
    "license": "MIT",
    "engines": {
        "vscode": "^1.28.0"
    },
    "categories": [
        "Snippets"
    ],
    "keywords": [
        "snippets",
        "react",
        "javascript",
        "typescript"
    ],
    "extensionKind": [
        "ui",
        "workspace"
    ],
    "capabilities": {
        "virtualWorkspaces": true,
        "untrustedWorkspaces": {
            "supported": true
        }
    },
    "contributes": {
        "snippets": [
            {
                "language": "javascript",
                "path": "./snippets.json"
            },
            {
                "language": "typescript",
                "path": "./snippets.json"
            },
            {
                "language": "javascriptreact",
                "path": "./snippets.json"
            },
            {
                "language": "typescriptreact",
                "path": "./snippets.json"
            }
        ]
    }
}
